		                <li><a md-button aria-label="meanu" href="#/tables/static"><i class="fa fa-angle-right"></i><span data-translate="STATIC_TABLES"></span></a></li>
		                <li><a md-button aria-label="meanu" href="#/tables/responsive"><i class="fa fa-angle-right"></i><span data-translate="RESPONSIVE_TABLES"></span> <span class="badge badge-info">2</span></a></li>
		                <li><a md-button aria-label="meanu" href="#/tables/dynamic"><i class="fa fa-angle-right"></i><span data-translate="DYNAMIC_TABLES"></span></a></li>
		            </ul>
		        </li>
		        <li>
		            <a md-button aria-label="meanu" href="#/form"><i class="zmdi zmdi-edit"></i><span data-translate="FORMS"></span></a>
		            <ul>
		                <li><a md-button aria-label="meanu" href="#/forms/layouts"><i class="fa fa-angle-right"></i><span data-translate="FORM_LAYOUTS"></span></a></li>
